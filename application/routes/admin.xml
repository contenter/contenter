<?xml version="1.0" encoding="utf-8"?>
<configdata>
    <routes>
        <AdminIndexIndex>
            <type>Zend_Controller_Router_Route_Static</type>
            <route>admin/index.html</route>
            <defaults>
                <module>admin</module>
                <controller>index</controller>
                <action>index</action>
            </defaults>
        </AdminIndexIndex>    
        <AdminAuthLogin>
            <type>Zend_Controller_Router_Route_Static</type>
            <route>admin/login.html</route>
            <defaults>
                <module>admin</module>
                <controller>auth</controller>
                <action>login</action>
            </defaults>
        </AdminAuthLogin>
        <AdminAuthLogout>
            <type>Zend_Controller_Router_Route_Static</type>
            <route>admin/logout.html</route>
            <defaults>
                <module>admin</module>
                <controller>auth</controller>
                <action>logout</action>
            </defaults>
        </AdminAuthLogout>
    </routes>
</configdata>