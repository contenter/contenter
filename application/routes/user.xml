<?xml version="1.0" encoding="utf-8"?>
<configdata>
    <routes>
        <UserRegisterRegister>
            <type>Zend_Controller_Router_Route_Static</type>
            <route>user/register.html</route>
            <defaults>
                <module>user</module>
                <controller>register</controller>
                <action>register</action>
            </defaults>
        </UserRegisterRegister>
        <UserAuthLogin>
            <type>Zend_Controller_Router_Route_Static</type>
            <route>user/login.html</route>
            <defaults>
                <module>user</module>
                <controller>auth</controller>
                <action>login</action>
            </defaults>
        </UserAuthLogin>
        <UserAuthLogout>
            <type>Zend_Controller_Router_Route_Static</type>
            <route>user/logout.html</route>
            <defaults>
                <module>user</module>
                <controller>auth</controller>
                <action>logout</action>
            </defaults>
        </UserAuthLogout>
        <UserIndexIndex>
            <type>FinalView_Controller_Router</type>
            <route>user/{:user_id/}index.html</route>
            <defaults>
                <module>user</module>
                <controller>index</controller>
                <action>index</action>
            </defaults>
        </UserIndexIndex>
        <UserRegisterConfirmation>
            <type>Zend_Controller_Router_Route</type>
            <route>user/role/:role/:email/resend-confirmation.html</route>
            <defaults>
                <module>user</module>
                <controller>register</controller>
                <action>confirmation</action>
            </defaults>
        </UserRegisterConfirmation>                                   
        <UserAuthForgotPassword>
            <type>Zend_Controller_Router_Route_Static</type>
            <route>user/forgot-password.html</route>
            <defaults>
                <module>user</module>
                <controller>auth</controller>
                <action>forgot-password</action>
            </defaults>
        </UserAuthForgotPassword>
        <UserAuthForgotPasswordMailSent>
            <type>Zend_Controller_Router_Route</type>
            <route>user/:hash/forgot-password-mail-sent.html</route>
            <defaults>
                <module>user</module>
                <controller>auth</controller>
                <action>forgot-password-mail-sent</action>
            </defaults>
        </UserAuthForgotPasswordMailSent>
        <UserPageIndex>
            <type>Zend_Controller_Router_Route_Static</type>
            <route>user/create-page.html</route>
            <defaults>
                <module>user</module>
                <controller>page</controller>
                <action>create-new</action>
            </defaults>
        </UserPageIndex>
        <UserPageSetUp>
            <type>Zend_Controller_Router_Route</type>
            <route>user/page/:page_id/setup-page.html</route>
            <defaults>
                <module>user</module>
                <controller>page</controller>
                <action>set-up</action>
            </defaults>
        </UserPageSetUp>
        <UserPageBuild>
            <type>Zend_Controller_Router_Route</type>
            <route>user/page/:page_id/build.html</route>
            <defaults>
                <module>user</module>
                <controller>page</controller>
                <action>build</action>
            </defaults>
        </UserPageBuild>
        <UserPageView>
            <type>Zend_Controller_Router_Route</type>
            <route>user/page/:page_id/view.html</route>
            <defaults>
                <module>user</module>
                <controller>page</controller>
                <action>view</action>
            </defaults>
        </UserPageView>
    </routes>
</configdata>
